			if(!MusicPlay){
				MusicButton[1].copyRect(0,0, SpecialParts, 110 ,MusicButton[1].height*2 );
			}
			
			MusicPlay = true;
		}
		else if(MusicPlayerObj.status == "stop")
		{//停止した MODEによってこの後の幼鳏なる
			
			if(MusicPlay == false)
			{//停止ボタンを押された
				
				MusicPlay = false;
				
				//CG MODEのI理を使用する
				CursorOffFunc( 49 );
				
				//Oタイマ`を停止
				MusicStatusCheck.enabled  = false;
				
				if(SuffuleMode == true)
				{//シャッフルリストを再度作成しておく
					MusicPlayerObj.createShuffle();
					
					SuffleCount = 0;
				}
				
				return;
			}
			
			
			
			
			if(SuffuleMode == true)
			{//シャッフルモ`ド
				
				//カウントをあげる
				++SuffleCount;
				
				if((MusicPlayerObj.playFileName.count-1) < SuffleCount)
				{//全て再生しKわったので停止
					
					if(RepeatMode == 2)
					{//Full Repeat だとシャッフルして再度^から
						
						//Oタイマ`を停止
						MusicStatusCheck.enabled  = false;
						
						//シャッフルリストを再度作成しておく
						MusicPlayerObj.createShuffle();
						
						SuffleCount = 0;
						
						//LIST BORDをす
						MusicCursorPrevFunc();
						
						NowSelectMusic = MusicPlayerObj.shuffleList[SuffleCount];
						
						NowSelectMusic %= MusicPlayerObj.playFileName.count;
						
						//xkカ`ソルを移婴筏皮く
						MusicCursorMoveFunc(true);
						
						//再度再生
						MusicPlayFunc(true);
						
					}else{
					 //通常停止
					 	
						MusicPlay = false;
						
						//再生ボタンをす
						CursorOffFunc( 49 );
						
						//Oタイマ`を停止
						MusicStatusCheck.enabled  = false;
						
						//シャッフルリストを再度作成しておく
						MusicPlayerObj.createShuffle();
					}
					
				}else{
					
					//LIST BORDをす
					MusicCursorPrevFunc();
					
					NowSelectMusic = MusicPlayerObj.shuffleList[SuffleCount];
					
					NowSelectMusic %= MusicPlayerObj.playFileName.count;
					
					//xkカ`ソルを移婴筏皮く
					MusicCursorMoveFunc(true);
					
					//再度再生
					MusicPlayFunc(true);
				}
			}
			else if(RepeatMode == 0)
			{//通常再生
				MusicPlay = false;
				
				//再生ボタンをす
				CursorOffFunc( 49 );
				
				//Oタイマ`を停止
				MusicStatusCheck.enabled  = false;
			}
			else if(RepeatMode == 2)
			{//Full Repeat
				
				//LIST BORDをす
				MusicCursorPrevFunc();
				
				//通常は次へ
				++NowSelectMusic;
				
				NowSelectMusic %= MusicPlayerObj.playFileName.count;
				
				//xkカ`ソルを移婴筏皮く
				MusicCursorMoveFunc(true);
				
				//再度再生
				MusicPlayFunc(true);
			}
		}
	}
	
	
	
	
	
	
	/*-----------------------------------------
	■ シャッフル INDEX取得v数
	-----------------------------------------*/
	function GetSuffleIndex(index)
	{//index はxkされた目なので、SuffleListから仕鳏筏匹ウントをずらす
		
		for(var i=0; i<MusicPlayerObj.playFileName.count; i++)
		{
			if(MusicPlayerObj.shuffleList[i] == index){
				
				SuffleCount = i;
				
				break;
			}
		}
		
		return MusicPlayerObj.shuffleList[SuffleCount];
	}
	
	
	
	
	
